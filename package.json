{
  "name": "typescript-ddd-template",
  "version": "1.0.0",
  "description": "typescript ddd template",
  "main": "./dist/apps/backOffice/backend/main.js",
  "scripts": {
    "start": "npm run start:backOffice:backend",
    "start:backOffice:backend": "NODE_ENV=production node ./dist/apps/backOffice/backend/main.js",
    "build:backOffice:backend": "tsc -p ./tsconfig.json",
    "build": "npm run build:backOffice:backend",
    "dev": "npm run dev:backOffice:backend",
    "dev:backOffice:backend": "ts-node-dev --watch ./src  --clear --ignore-watch node_modules ./src/apps/backOffice/backend/main.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json}' --config .prettierrc.json",
    "check-types": "tsc --noEmit -p ./tsconfig.json",
    "test": "echo \"Test\" "
  },
  "keywords": [],
  "author": "LiasCode",
  "dependencies": {
    "@types/uuid": "9.0.7",
    "axios": "1.6.2",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "mysql": "2.18.1",
    "redis": "4.6.12",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/mysql": "2.15.25",
    "@types/node": "20.10.5",
    "concurrently": "8.2.2",
    "prettier": "3.1.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.3.3"
  }
}
